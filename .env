# TikTok Ads API Configuration
# Get these credentials from TikTok For Business Developer Portal
# https://business-api.tiktok.com/portal/

# Required: TikTok App credentials
TIKTOK_APP_ID=
TIKTOK_APP_SECRET=

# Required: Access token for API requests
# Get this through OAuth flow or from TikTok developer dashboard
TIKTOK_ACCESS_TOKEN=

# Required: Advertiser account ID
# This is the ID of the TikTok Ads account you want to manage
TIKTOK_ADVERTISER_ID=

# Optional: Logging configuration
LOG_LEVEL=INFO
LOG_FILE=tiktok_ads_mcp.log

# Optional: API Rate limiting
RATE_LIMIT_REQUESTS_PER_HOUR=900
RATE_LIMIT_CONCURRENT_REQUESTS=8

# Optional: Default report settings
DEFAULT_REPORT_TIMEOUT_MINUTES=10
DEFAULT_DATE_RANGE=last_7_days

# Development settings
DEVELOPMENT_MODE=false
DEBUG_API_REQUESTS=false